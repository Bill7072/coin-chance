doctype html
html(lang="en")
    head
        title= title
        script(type='text/javascript').
            $(function() {
            });
    body
        p 
            span rollId
            span #{rollId}
        
        p 
            span userId
            span #{userId}

        p 
            span multiplier
            span #{multiplier}

        p 
            span stake
            span #{stake}

        p 
            span profit
            span #{profit}

        p 
            span chance
            span #{chance}

        p 
            span target
            span #{(isHighGuess) ? ">" : "<"}
            span #{target}

        p 
            span lucky
            span #{lucky}

        p 
            span didWin 
            span #{didWin}

        p 
            span serverSeedHash
            span #{serverSeedHash}

        p 
            span serverSeed
            span #{serverSeed}

        p 
            span clientSeed
            span #{clientSeed}

        p 
            span nonce
            span #{nonce}
        #agplStuff
            a#agplLink(href="#{srcLink}")
                img#agplLogo(src="/images/agplv3-155x51.png")
